<view 
    class="com-wrapper" 
    bindtouchmove="touchmove" 
    bindtouchend="touchend" 
    bindtouchstart="touchstart"
>
    <view class="mask mask-top"></view>
    <view class="mask mask-bottom"></view>
    <view 
        id="flex-wrapper"
        class="flex-wrapper"
        animation="{{ animationData }}"
        style="transform: translate3d(0, {{ transYValue }}px, 0);"
    >
        <view class="flex-item {{ activeIndex === index ? 'active' : '' }}" wx:for="{{ range }}" wx:key="{{ index }}">
            {{ item }}
        </view>
    </view>
</view>